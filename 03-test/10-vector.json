{
  "name": "vector",
  "description": "Setting for test vectors (focus on on-line vectors)",
  "kind": "test",
  "spec": {
        "online": ["vector-party", "vector-contact",
          "vector-partycontact", "vector-partycontact-02",
          "vector-account", "vector-partyaccount", "vector-partyaccount-02"
        ],
        "tests": {
          "vector-party": {
            "feature-set": "01-basic_party",
            "entities": ["party_id"],
            "features": ["party_id", "party_establishment", "party_type", "party_gender","party_education", "party_familystatus",
            "party_nchild", "party_industry","party_industryposition", "party_income", "party_incomehousehold"]
          },
          "vector-contact": {
            "feature-set": "02-basic_contact",
            "entities": ["party_id"],
            "features": ["party_id", "contact_id", "contact_email", "contact_phone", "contact_state"]
          },
          "vector-partycontact": {
            "feature-set": "02-basic_contact",
            "entities": ["party_id"],
            "features": ["party_id", "contact_email", "contact_phone", "contact_state"]
          },
          "vector-partycontact-02": {
            "feature-set": "02-basic_contact",
            "entities": ["party_id"],
            "features": ["contact-id", "contact-email", "contact-phone", "contact-state"]
          },
          "vector-account": {
            "feature-set": "04-basic-account",
            "entities": ["party_id"],
            "features": ["party_id", "account-id","account-type", "account-state", "account-createdate", "account-nonactivedate"]
          },
          "vector-partyaccount": {
            "feature-set": "04-basic-account",
            "entities": ["party_id"],
            "features": ["party_id", "account-type", "account-state"]
          },
          "vector-partyaccount-02": {
            "feature-set": "04-basic-account",
            "entities": ["party_id", "account-id"],
            "features": ["party_id", "account-id", "account-createdate", "account-nonactivedate"]
          }
        }
  }
}