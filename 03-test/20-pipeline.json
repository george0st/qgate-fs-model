          "basic-account": {
            "filter": "(event['account-state'] == 'active')",
            "extend": "({'baccount': 'account'})",
            "calc": "",
            "aggregation": ""
          },
          "basic-transaction":
          {
            "filter": "(event['transaction-direction'] == 'Incoming' and event['transaction-type'] == 'Standard')",
            "extend": "({'btransaction': 'transaction'})",
            "calc": "",
